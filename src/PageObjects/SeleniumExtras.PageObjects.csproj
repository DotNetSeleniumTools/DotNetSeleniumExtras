<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net45;net40;net35;netstandard2.0</TargetFrameworks>
    <AssemblyName>SeleniumExtras.PageObjects</AssemblyName>
    <RootNamespace>SeleniumExtras</RootNamespace>
    <AssemblyVersion>3.11.1.0</AssemblyVersion>
    <FileVersion>3.11.1.0</FileVersion>
    <Version>3.11.1-beta</Version>
    <PackageId>DotNetSeleniumExtras.PageObjects.Core</PackageId>
    <Authors>Dreamescaper</Authors>
    <Description>Fork of DotNetSeleniumExtras with enabled dotnet core support and other features</Description>
    <Copyright>Copyright 2018, Software Freedom Conservancy</Copyright>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/Dreamescaper/DotNetSeleniumExtras/</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Selenium.WebDriver" Version="3.11.1" />
  </ItemGroup>

  <ItemGroup Condition="'$(TargetFramework)' == 'netstandard2.0'">
    <PackageReference Include="System.Reflection.DispatchProxy" Version="4.4.0"/>
  </ItemGroup>

</Project>
